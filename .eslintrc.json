{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "react-app", 
        "react-app/jest"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "plugins": ["react", "only-warn"],
    "rules": {
        // Add any custom rules here
        "react/prop-types": "off", 
        "react/react-in-jsx-scope": "off"
    }
}
